@baseUrl = http://localhost:3000

### Create an admin
POST {{baseUrl}}/auth/register
Content-Type: application/json

{
    "name": "John Doe",
    "email": "john.doe@gmail.com",
    "password": "password"
}

### Create a company
POST {{baseUrl}}/auth/register-company
Content-Type: application/json

{
    "name": "superCompany",
    "email": "contact@supercompany.be",
    "password": "password",
    "template" : [
      1, 2, 3, 4, 5
    ]
}

### Login
POST {{baseUrl}}/auth/login
Content-Type: application/json

{
  "email": "john.doe@gmail.com",
  "password": "password"
}

